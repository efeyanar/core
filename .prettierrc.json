{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "printWidth": 180,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "pluginSearchDirs": false,
  "tailwindConfig": "./tailwind.config.ts",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
